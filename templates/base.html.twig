<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        <link rel="preconnect" href="https://fonts.googleapis.com"> 
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin> 
        <link href="https://fonts.googleapis.com/css2?family=Butterfly+Kids&family=Montserrat&family=Rubik+Puddles&family=Sacramento&display=swap" rel="stylesheet">
	    <link rel="icon" href="{{ '/images/' ~ ('téléchargement.png') }}" type="image/x-icon" src="" >
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
       
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            {# {{ encore_entry_link_tags('blog') }} #}
            {{ encore_entry_link_tags('faq') }}
            {{ encore_entry_link_tags('oneGame') }}
        {% endblock %}
        
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            {# {{ encore_entry_script_tags('blog') }} #}
            {{ encore_entry_script_tags('faq') }}
            {{ encore_entry_script_tags('oneGame') }}
        {% endblock %}
        
    </head>
    <body>

        {{ include("header.html.twig") }}
        {{ include("menu.html.twig") }}

        {% block start_container %}
            <div class="container">
        {% endblock %}

            {% block body %}
            {% endblock %}

        {% block end_container %}
            </div>
        {% endblock %}

        {{ include("footer.html.twig") }}
	
	<script src="{{asset('../assets/test.js')}}"></script>
    </body>
</html>
