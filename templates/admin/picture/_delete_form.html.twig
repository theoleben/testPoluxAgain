<form method="post" action="{{ path('app_admin_picture_delete', {'id': picture.id}) }}" onsubmit="return confirm('Etes-vous sûr de vouloir supprimer cette photo ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="btn">Supprimer la photo n°{{picture.id}}</button>
</form>
