<form method="post" action="{{ path('app_admin_picture_delete', {'id': picture.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cette photo?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
