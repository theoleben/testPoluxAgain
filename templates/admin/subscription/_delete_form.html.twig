<form method="post" action="{{ path('app_admin_subscription_delete', {'id': subscription.id}) }}" onsubmit="return confirm('Etes-vous certain de vouloir supprimer cet abonnement ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ subscription.id) }}">
    <button class="btn sm-btn btn-secondary">Supprimer</button><br>
</form>
